<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="Done" prefHeight="400.0" prefWidth="602.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SettingController">
   <children>
      <Label layoutX="213.0" layoutY="3.0" prefHeight="45.0" prefWidth="184.0" text="gamePlay.Game Setting">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font></Label>
      <VBox layoutX="138.0" layoutY="42.0" prefHeight="307.0" prefWidth="297.0">
         <children>
            <Label prefHeight="66.0" prefWidth="127.0" text="BoardSize" underline="true">
               <font>
                  <Font name="System Bold Italic" size="16.0" />
               </font></Label>
            <HBox prefHeight="84.0" prefWidth="175.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <TextField fx:id="inputBoardSize" prefHeight="84.0" prefWidth="43.0" text="8" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label prefHeight="39.0" prefWidth="79.0" text="playerA:">
                           <font>
                              <Font name="System Bold Italic" size="16.0" />
                           </font></Label>
                        <TextField fx:id="inputPl1Col" prefHeight="84.0" prefWidth="43.0" text="Black" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label prefHeight="25.0" prefWidth="90.0" text="playerB:">
                           <font>
                              <Font name="System Bold Italic" size="16.0" />
                           </font></Label>
                        <TextField fx:id="inputPl2Col" prefHeight="84.0" prefWidth="43.0" text="Yellow" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <VBox prefHeight="89.0" prefWidth="157.0">
               <children>
                  <Label fx:id="blackScore1" prefHeight="36.0" prefWidth="208.0" text="IsPLayer1Starting:" underline="true">
                     <font>
                        <Font name="System Bold Italic" size="16.0" />
                     </font></Label>
                  <CheckBox fx:id="inputIsBlackStart" mnemonicParsing="false" onAction="#handleTextTurn" prefHeight="36.0" prefWidth="59.0" selected="true" wrapText="true">
                     <VBox.margin>
                        <Insets left="50.0" />
                     </VBox.margin>
                  </CheckBox>
               </children>
            </VBox>
            <Button fx:id="done" mnemonicParsing="false" onAction="#doneButtonClicked" prefHeight="53.0" prefWidth="251.0" text="Done" />
         </children>
      </VBox>
   </children>
</AnchorPane>
